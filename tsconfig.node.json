{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*",
   "src/main/**/*",
    "src/preload/**/*",
   "src/types/**/*",
   "src/utils/**/*"

  ],
  "compilerOptions": {
    "composite": true,
    "outDir": "out",
    "types": ["electron-vite/node"
  ],
  "baseUrl": ".",
  "paths": {
  
    "types/*": ["src/types/*"],
    "utils/*": ["src/utils/*"]
  }
  }
}
